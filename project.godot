; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Paths Of Jjaro"
config/description="Open Source remake of Bungie's Marathon"
run/main_scene="res://assets/ui/menu/MainMenu.tscn"
config/features=PackedStringArray("4.0")
config/icon="res://icon.png"

[autoload]

Gamestate="*res://data/singletons/gamestate.gd"
Pooling="*res://data/singletons/pooling.gd"
GlobalSettings="*res://data/singletons/global_settings.gd"
MaterialPool="*res://data/singletons/material_pool.gd"
InputMaping="*res://data/singletons/input_maping.gd"

[debug]

gdscript/warnings/enable=false
multirun/number_of_windows=4
multirun/window_distance=200
multirun/add_custom_args=true
multirun/first_window_args="server"
multirun/other_window_args="client"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/vsync/vsync_mode=false

[dotnet]

project/assembly_name="Paths Of Jjaro"

[editor_plugins]

enabled=PackedStringArray("res://addons/goap/plugin.cfg", "res://addons/multirun/plugin.cfg", "res://addons/sade/plugin.cfg")

[global]

fps=false

[importer_defaults]

ogg_vorbis={
"loop": false,
"loop_offset": 0
}

